{
  "name": "mina-zk-rollup",
  "version": "0.1.4",
  "description": "",
  "author": "",
  "license": "Apache-2.0",
  "keywords": [],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "test:coverage": "nyc npm run test",
    "test2": "nyc --all --reporter=html --reporter=text  mocha --require ts-node/register ./tests/**/*.test.ts --exit",
    "build": "tsc -p tsconfig.json",
    "demo": "npx tsc && node ./build/src/demo.js",
    "operator:demo": "npx tsc && node ./build/src/rollup_operator/index.js",
    "format": "prettier --write --ignore-unknown **/*",
    "vue:dev": "vite",
    "vue:build": "vue-tsc --noEmit && vite build",
    "vue:preview": "vite preview"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "chai": "^4.3.6",
    "lint-staged": "^11.0.1",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "postcss": "^8.4.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^3.0.20",
    "ts-node": "^10.5.0",
    "typescript": "^4.3.5",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@o1labs/client-sdk": "^1.0.1",
    "config": "^3.3.7",
    "express": "^4.17.2",
    "snarkyjs": "^0.2.0",
    "vue": "^3.2.25"
  }
}
