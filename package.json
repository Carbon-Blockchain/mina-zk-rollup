{
  "name": "mina-zk-rollup",
  "version": "0.1.4",
  "description": "",
  "author": "",
  "license": "Apache-2.0",
  "keywords": [
    "mina-snapp",
    "mina-zk-app",
    "mina-dapp"
  ],
  "type": "module",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "demo": "npx tsc && node ./build/src/demo.js",
    "operator:demo": "npx tsc && node ./build/src/rollup_operator/index.js",
    "build": "tsc -p tsconfig.json",
    "coverage": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads --no-warnings node_modules/jest/bin/jest.js --coverage",
    "format": "prettier --write --ignore-unknown **/*",
    "prepare": "husky install",
    "test": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads --no-warnings node_modules/jest/bin/jest.js",
    "testw": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads --no-warnings node_modules/jest/bin/jest.js --watch",
    "lint": "npx eslint src/* --fix"
  },
  "lint-staged": {
    "**/*": [
      "eslint src/* --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "snarkyjs": "^0.2.0",
    "@o1labs/client-sdk": "^1.0.1",
    "config": "^3.3.7",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.7.0",
    "eslint-plugin-snarkyjs": "^0.1.0",
    "husky": "^7.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2",
    "@types/express": "^4.17.13",
    "@types/pg": "^8.6.4"
  }
}
